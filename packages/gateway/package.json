{
  "name": "@llmops/gateway",
  "version": "0.1.3",
  "description": "AI gateway for LLMOps (forked from Portkey)",
  "type": "module",
  "license": "Apache-2.0",
  "homepage": "https://llmops.build",
  "bugs": {
    "url": "https://github.com/llmops-build/llmops/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/llmops-build/llmops.git",
    "directory": "packages/gateway"
  },
  "author": "chtushar",
  "keywords": [
    "AI gateway",
    "LLMOps",
    "LLM",
    "OpenAI"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown",
    "clean": "rm -rf dist",
    "dev": "tsdown --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@aws-crypto/sha256-js": "^5.2.0",
    "@cfworker/json-schema": "^4.0.3",
    "@smithy/signature-v4": "^2.1.1",
    "async-retry": "^1.3.3",
    "avsc": "^5.7.7",
    "ioredis": "^5.8.0",
    "hono": "^4.10.7",
    "jose": "^6.0.11",
    "patch-package": "^8.0.1",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230518.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/async-retry": "^1.4.5",
    "@types/node": "20.8.3",
    "@types/ws": "^8.5.12",
    "portkey-ai": "^1.9.1",
    "prettier": "3.2.5",
    "rollup": "^4.9.1",
    "rollup-plugin-copy": "^3.5.0",
    "tsx": "^4.7.0",
    "typescript-eslint": "^8.1.0",
    "vitest": "^3.1.4",
    "wrangler": "^3.97.0"
  }
}
