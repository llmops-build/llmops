{
  "name": "@llmops/cli",
  "version": "0.1.2-beta.3",
  "type": "module",
  "description": "LLMOps CLI - A pluggable LLMOps toolkit for TypeScript teams",
  "license": "Apache-2.0",
  "homepage": "https://llmops.build",
  "bugs": {
    "url": "https://github.com/llmops-build/llmops/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/llmops-build/llmops.git",
    "directory": "packages/cli"
  },
  "author": "chtushar",
  "files": [
    "dist"
  ],
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": "./dist/index.mjs",
  "bin": {
    "cli": "./dist/index.mjs",
    "llmops": "./dist/index.mjs"
  },
  "publishConfig": {
    "access": "public",
    "executableFiles": [
      "./dist/index.mjs"
    ]
  },
  "scripts": {
