    "clean": "rm -rf dist",
    "dev": "tsdown --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "llmops",
    "cli",
    "ai",
    "llm",
    "typescript"
  ],
  "dependencies": {
    "@drizzle-team/brocli": "^0.11.0",
    "@llmops/core": "workspace:^",
    "@llmops/sdk": "workspace:^",
    "c12": "^3.3.2",
    "chalk": "^5.6.2",
    "kysely": "^0.28.8",
    "prompts": "^2.4.2",
    "yocto-spinner": "^1.0.0"
  },
  "devDependencies": {
    "@types/pg": "^8.15.6",
    "@types/prompts": "^2.4.9",
    "pg": "^8.16.3"
  }
