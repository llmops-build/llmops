{
  "name": "@llmops/core",
  "version": "0.2.12-beta.1",
  "description": "Core LLMOps functionality and utilities",
  "type": "module",
  "license": "Apache-2.0",
  "homepage": "https://llmops.build",
  "bugs": {
    "url": "https://github.com/llmops-build/llmops/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/llmops-build/llmops.git",
    "directory": "packages/core"
  },
  "author": "chtushar",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./db": {
      "import": {
        "types": "./dist/db/index.d.mts",
        "default": "./dist/db/index.mjs"
      },
      "require": {
        "types": "./dist/db/index.d.cts",
        "default": "./dist/db/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown",
    "clean": "rm -rf dist",
    "dev": "tsdown --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@better-auth/utils": "^0.3.0",
    "@llmops/gateway": "workspace:^",
    "@neondatabase/serverless": "^1.0.2",
    "better-auth": "^1.4.10",
    "hono": "^4.10.7",
    "kysely": "^0.28.8",
    "kysely-neon": "^2.0.2",
    "pino": "^10.1.0"
  },
  "devDependencies": {
    "@types/ws": "^8.18.1"
  }
}
