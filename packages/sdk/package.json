{
  "name": "@llmops/sdk",
  "version": "0.2.11-beta.2",
  "description": "An LLMOps toolkit for TypeScript applications",
  "type": "module",
  "license": "Apache-2.0",
  "homepage": "https://llmops.build",
  "bugs": {
    "url": "https://github.com/llmops-build/llmops/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/llmops-build/llmops.git",
    "directory": "packages/sdk"
  },
  "keywords": [
    "llmops",
    "ai",
    "llm",
    "typescript",
    "openai"
  ],
  "author": "chtushar",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./express": {
      "import": {
        "types": "./dist/express.d.mts",
        "default": "./dist/express.mjs"
      },
      "require": {
        "types": "./dist/express.d.cts",
        "default": "./dist/express.cjs"
      }
    },
    "./hono": {
      "import": {
        "types": "./dist/hono.d.mts",
        "default": "./dist/hono.mjs"
      },
      "require": {
        "types": "./dist/hono.d.cts",
        "default": "./dist/hono.cjs"
      }
    },
    "./convex": {
      "import": {
        "types": "./dist/convex.d.mts",
        "default": "./dist/convex.mjs"
      },
      "require": {
        "types": "./dist/convex.d.cts",
        "default": "./dist/convex.cjs"
      }
    },
    "./edge": {
      "import": {
        "types": "./dist/edge.d.mts",
        "default": "./dist/edge.mjs"
      },
      "require": {
        "types": "./dist/edge.d.cts",
        "default": "./dist/edge.cjs"
      }
    },
    "./*": "./*"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown",
    "clean": "rm -rf dist",
    "dev": "tsdown --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@llmops/app": "workspace:^",
    "@llmops/core": "workspace:^"
  },
  "peerDependencies": {
    "convex": "^1.0.0"
  },
  "peerDependenciesMeta": {
    "convex": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "hono": "^4.7.0"
  }
}
